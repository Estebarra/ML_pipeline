name: clean_data
conda_env: conda.yml

entry_points:
  main:
    parameters:

      input_artifact:
        description: "Input file"
        type: string

      output_artifact:
        description: "Output file"
        type: string

      output_type:
        description: "Output type"
        type: string

      output_description:
        description: "File processed by applying cleaning steps"
        type: string


    command: >-
        python run.py  --input_artifact {input_artifact}  --output_artifact {output_artifact}  --output_type {output_type}  --output_description {output_description}