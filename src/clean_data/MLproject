name: clean_data

entry_points:
  main:
    parameters:

      input_artifact:
        description: "Archivo de entrada"
        type: string

      output_artifact:
        description: "Archivo de salida"
        type: string

      output_type:
        description: "Tipo de salida"
        type: string

      output_description:
        description: "Archivo procesado por la aplicaciÃ³n de pasos de limpieza"
        type: string


    command: >-
        python run.py --input_artifact {input_artifact} \
                      --output_artifact {output_artifact} \
                      --output_type {output_type} \
                      --output_description {output_description}