name: test_model

entry_points:
  main:
    parameters:

      mlflow_model:
        description: Modelo MLflow a probar
        type: string

      test_dataset:
        description: Conjunto de datos de prueba
        type: string

    command: >-
      python run.py --mlflow_model {mlflow_model} \
                    --test_dataset {test_dataset}